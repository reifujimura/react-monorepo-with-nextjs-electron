{
  "extends": "../../node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true
  },
  "include": [
    "next-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
